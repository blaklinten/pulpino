#ifndef DATASTRUCT_H
#define DATASTRUCT_H

#include "pulpino.h"

#define MATRIX_ELEMENT 32

// Accelerator base address
#define HWACC_BASEADDR 0x100000

// UART interrupt
#define UART_IRQ 0x60

struct Matrises {
  // The two matrices
  __uint8_t matrixA[MATRIX_ELEMENT][MATRIX_ELEMENT];
  __uint8_t matrixB[MATRIX_ELEMENT][MATRIX_ELEMENT];
};

#endif
